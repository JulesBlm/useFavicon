{
  "name": "react-usefavicon",
  "description": "useFavicon - a React Hook to modify the favicon",
  "author": "Jules Blom (https://twitter.com/MegaJules)",
  "private": false,
  "homepage": "https://jules.engineer/usefavicon",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/JulesBlm/useFavicon.git"
  },
  "sideEffects": false,
  "main": "./dist/usefavicon.js",
  "browser": "./dist/usefavicon.umd.js",
  "umd:main": "./dist/usefavicon.umd.js",
  "jsnext:main": "./dist/usefavicon.modern.js",
  "module": "./dist/usefavicon.module.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "favicon",
    "react",
    "react-hook"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/JulesBlm"
  },
  "scripts": {
    "build": "rimraf dist && npm run test && npm run build:react",
    "build:react": "microbundle --define process.env.NODE_ENV=production --name usefavicon --no-compress --output dist/",
    "build:ci": "microbundle --define process.env.NODE_ENV=production --name usefavicon --output dist/",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ],
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.3.1",
    "microbundle": "^0.13.3",
    "prettier": "2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "files": [
    "src",
    "dist"
  ]
}
