{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "plugin:react-hooks/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "react/prop-types": false,
  "env": {
    "browser": true,
    "mocha": true,
    "es6": true
  },
  "plugins": ["prettier"]
}
